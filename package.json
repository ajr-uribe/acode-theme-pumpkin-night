{
  "name": "halloween-pumpkin-night",
  "version": "1.0.0",
  "description": "Acode theme for Halloween",
  "main": "src/main.js",
  "scripts": {
    "sync": "rm -rf ~/acode-theme/* && rsync -rv --delete --exclude node_modules ~/storage/shared/www/acode-theme/* ~/acode-theme/ && cd ~/acode-theme && npm i && npm run build && npm run sync-r",
    "sync-r": "rsync -rv --delete --exclude node_modules ~/acode-theme/*  ~/storage/shared/www/acode-theme/",
    "dev": "node esbuild.config.mjs --serve",
    "build": "node esbuild.config.mjs"
  },
  "keywords": [],
  "author": "JOBIANSTECHIE",
  "license": "ISC",
  "packageManager": "pnpm@10.11.1",
  "devDependencies": {
    "esbuild": "^0.25.5",
    "esbuild-sass-plugin": "^3.3.1",
    "jszip": "^3.10.1"
  },
  "browserslist": "cover 100%,not android < 5",
  "dependencies": {
    "html-tag-js": "^2.3.5"
  }
}